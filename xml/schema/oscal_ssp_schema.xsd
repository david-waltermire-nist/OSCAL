            <xs:element minOccurs="0" ref="oscal-ssp:format"/>
            <xs:element minOccurs="0" ref="oscal-ssp:date"/>
            <xs:element minOccurs="0" ref="oscal-ssp:version"/>
            <xs:element minOccurs="0" ref="oscal-ssp:attachment-type"/>
            <xs:element minOccurs="0" ref="oscal-ssp:base64"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
