            <xs:element minOccurs="0" ref="oscal-ssp:org-name"/>
            <xs:element minOccurs="0" ref="oscal-ssp:short-name"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:org-id"/>
            <xs:element minOccurs="0" ref="oscal-ssp:address"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:email"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:phone"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:url"/>
            <xs:element minOccurs="0" ref="oscal-ssp:notes"/>
