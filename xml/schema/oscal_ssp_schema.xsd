<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:oscal-ssp="urn:OSCAL-SSP-metaschema"
           xmlns:oscal-prose="urn:OSCAL-SSP-metaschema"
           elementFormDefault="qualified"
           targetNamespace="urn:OSCAL-SSP-metaschema"><!--OSCAL System Security Plan (SSP) Format--><!--short name: oscal-ssp--><!--The OSCAL Control SSP format can be used to describe the information typically specified in a FedRAMP system security plan. The root of the Control Catalog format is system_security_plan.--><!--An XML Schema is provided for the OSCAL SSP XML model.-->
   <xs:element name="system-security-plan">
      <xs:annotation>
         <xs:documentation>
            <b>System Security Plan (SSP): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:metadata"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:import"/>
            <xs:element minOccurs="0" ref="oscal-ssp:system-characteristics"/>
            <xs:element minOccurs="0" ref="oscal-ssp:system-implementation"/>
            <xs:element minOccurs="0" ref="oscal-ssp:control-implementation"/>
            <xs:element minOccurs="0" ref="oscal-ssp:references"/>
            <xs:element minOccurs="0" ref="oscal-ssp:attachment"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID" use="required">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="system-characteristics">
      <xs:annotation>
         <xs:documentation>
            <b>System Characteristics: </b>Contains the characteristics of the system, such as its name, purpose, and
			security impact level.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:system-id"/>
            <xs:element minOccurs="1" ref="oscal-ssp:system-name"/>
            <xs:element minOccurs="0" ref="oscal-ssp:system-name-short"/>
            <xs:element minOccurs="0" ref="oscal-ssp:description"/>
            <xs:element minOccurs="1" ref="oscal-ssp:security-sensitivity-level"/>
            <xs:element minOccurs="0" ref="oscal-ssp:system-information"/>
            <xs:element minOccurs="0" ref="oscal-ssp:security-impact-level"/>
            <xs:element minOccurs="0" ref="oscal-ssp:security-eauth"/>
            <xs:element minOccurs="0" ref="oscal-ssp:status"/>
            <xs:element minOccurs="0" ref="oscal-ssp:status-other-description"/>
            <xs:element minOccurs="0" ref="oscal-ssp:deployment-model"/>
            <xs:element minOccurs="0" ref="oscal-ssp:deployment-model-other-description"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:service-model"/>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:service-model-other-description"/>
            <xs:element minOccurs="0" ref="oscal-ssp:leveraged-authorizations"/>
            <xs:element minOccurs="0" ref="oscal-ssp:authorization-boundary"/>
            <xs:element minOccurs="0" ref="oscal-ssp:network-architecture"/>
            <xs:element minOccurs="0" ref="oscal-ssp:data-flow"/>
            <xs:element minOccurs="0" ref="oscal-ssp:users"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="system-id">
      <xs:annotation>
         <xs:documentation>
            <b>System Identification: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:attribute name="type" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Type: </b>Type designates the type of information provided.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="system-name">
      <xs:annotation>
         <xs:documentation>
            <b>System Name (Full): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="system-name-short">
      <xs:annotation>
         <xs:documentation>
            <b>System Name (Short): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="description">
      <xs:annotation>
         <xs:documentation>
            <b>Description: </b>A free-text description of the system.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="oscal-ssp:prose" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="security-sensitivity-level">
      <xs:annotation>
         <xs:documentation>
            <b>: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="system-information">
      <xs:annotation>
         <xs:documentation>
            <b>System Information: </b>Contains details about all types of information stored, processed, or
			transmitted by the system, such as privacy information, and NIST 800-60 information
			types.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:information-type"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:designations"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="designations">
      <xs:annotation>
         <xs:documentation>
            <b>System Information Designations: </b>Used to enumerate all special information type designations that may be required by a specific organization or compliance regiem, such as privacy impact information (PII), or payment card information (PCI).</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:designation"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="designation">
      <xs:annotation>
         <xs:documentation>
            <b>System Information Designation: </b>Used when a specific organization or compliance regiem requires certain information types to be explicitly cited, such as privacy impact information (PII), or payment card information (PCI).</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:title"/>
            <xs:element minOccurs="0" ref="oscal-ssp:declaration"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:qualifiers"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="declaration" type="xs:boolean">
      <xs:annotation>
         <xs:documentation>
            <b>Declaration: </b>Identifies whether a specific designation applies to this system.</xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:element name="qualifiers">
      <xs:annotation>
         <xs:documentation>
            <b>Information Designation Qualifiers: </b>A list of questions about the system that contribute to the establishment of a designation, such as the FedRAMP privacy threshold analysis questions.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:qualifier"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="qualifier">
      <xs:annotation>
         <xs:documentation>
            <b>Individual Information Designation Qualifier: </b>An individual questions about the system that contributes to the establishment of a designation, such as a FedRAMP privacy threshold analysis question.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:qual-question"/>
            <xs:element minOccurs="0" ref="oscal-ssp:qual-response"/>
            <xs:element minOccurs="0" ref="oscal-ssp:qual-notes"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="qual-question">
      <xs:annotation>
         <xs:documentation>
            <b>Qualifier Question: </b>A question that must be answered about this system.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="qual-response" type="xs:boolean">
      <xs:annotation>
         <xs:documentation>
            <b>Qualifier Question Response: </b>The response to the question.</xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:element name="qual-notes">
      <xs:annotation>
         <xs:documentation>
            <b>Qualifier Question Response Notes: </b>Additional information about the response to the question.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="information-type">
      <xs:annotation>
         <xs:documentation>
            <b>Information Type: </b>Contains details about one type of information stored, processed, or
			transmitted by the system, such as privacy information, and NIST 800-60 information
			types.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:description"/>
            <xs:element minOccurs="0" ref="oscal-ssp:confidentiality-impact"/>
            <xs:element minOccurs="0" ref="oscal-ssp:integrity-impact"/>
            <xs:element minOccurs="0" ref="oscal-ssp:availability-impact"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="nist-id" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>NIST 800-60 Information Type Identifier: </b>Pointer to the information type in NIST SP 800-60.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="confidentiality-impact">
      <xs:annotation>
         <xs:documentation>
            <b>Confidentiality Impact Level: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:base"/>
            <xs:element minOccurs="0" ref="oscal-ssp:selected"/>
            <xs:element minOccurs="0" ref="oscal-ssp:adjustment-justification"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="integrity-impact">
      <xs:annotation>
         <xs:documentation>
            <b>Integrity Impact Level: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:base"/>
            <xs:element minOccurs="0" ref="oscal-ssp:selected"/>
            <xs:element minOccurs="0" ref="oscal-ssp:adjustment-justification"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="availability-impact">
      <xs:annotation>
         <xs:documentation>
            <b>Availability Impact Level: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:base"/>
            <xs:element minOccurs="0" ref="oscal-ssp:selected"/>
            <xs:element minOccurs="0" ref="oscal-ssp:adjustment-justification"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="base">
      <xs:annotation>
         <xs:documentation>
            <b>Base Level (Confidentiality, Integrity, or Availability): </b>The prescribed base (Confidentiality, Integrity, or Availability) security
			impact level.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="selected">
      <xs:annotation>
         <xs:documentation>
            <b>Selected Level (Confidentiality, Integrity, or Availability): </b>The selected (Confidentiality, Integrity, or Availability) security impact
			level.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="adjustment-justification">
      <xs:annotation>
         <xs:documentation>
            <b>Adjustment Justification: </b>If the selected security level is different from the base security level, this
			contains the justificaiton for the change.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="security-impact-level">
      <xs:annotation>
         <xs:documentation>
            <b>Security Impact Level: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:security-objective-confidentiality"/>
            <xs:element minOccurs="0" ref="oscal-ssp:security-objective-integrity"/>
            <xs:element minOccurs="0" ref="oscal-ssp:security-objective-availability"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="security-objective-confidentiality">
      <xs:annotation>
         <xs:documentation>
            <b>Security Objective (Confidentiality): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="security-objective-integrity">
      <xs:annotation>
         <xs:documentation>
            <b>Security Objective (Integrity): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="security-objective-availability">
      <xs:annotation>
         <xs:documentation>
            <b>Security Objective (Availability): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="security-eauth">
      <xs:annotation>
         <xs:documentation>
            <b>Security Electronic Authetication Level: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:security-auth-ial"/>
            <xs:element minOccurs="0" ref="oscal-ssp:security-auth-aal"/>
            <xs:element minOccurs="0" ref="oscal-ssp:security-auth-fal"/>
            <xs:element minOccurs="0" ref="oscal-ssp:security-eauth-level"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="security-auth-ial">
      <xs:annotation>
         <xs:documentation>
            <b>Security Authentication (Identity Assurance Level): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="security-auth-aal">
      <xs:annotation>
         <xs:documentation>
            <b>Security Authentication (Authenticator Assurance Level): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="security-auth-fal">
      <xs:annotation>
         <xs:documentation>
            <b>Security Authentication (Federation Assurance Level): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="security-eauth-level">
      <xs:annotation>
         <xs:documentation>
            <b>Security eAuthentication Level: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="status">
      <xs:annotation>
         <xs:documentation>
            <b>Status: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="status-other-description">
      <xs:annotation>
         <xs:documentation>
            <b>Status Description (if Other): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="deployment-model">
      <xs:annotation>
         <xs:documentation>
            <b>Deployment Model: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="deployment-model-other-description">
      <xs:annotation>
         <xs:documentation>
            <b>Deployment Model Description (if Other): </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="service-model">
      <xs:annotation>
         <xs:documentation>
            <b>Service Model: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="service-model-other-description">
      <xs:annotation>
         <xs:documentation>
            <b>Service Model Description Other: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="leveraged-authorizations">
      <xs:annotation>
         <xs:documentation>
            <b>Leveraged Authorizations: </b>All leveraged authorizations for this system.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:leveraged-authorization"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="leveraged-authorization">
      <xs:annotation>
         <xs:documentation>
            <b>Leveraged Authorization: </b>The details for a single leveraged authorization</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:leveraged-authorization-name"/>
            <xs:element minOccurs="0" ref="oscal-ssp:leveraged-authorization-service-provider"/>
            <xs:element minOccurs="0" ref="oscal-ssp:leveraged-authorization-date-granted"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="leveraged-authorization-name">
      <xs:annotation>
         <xs:documentation>
            <b>Leveraged system Name: </b>Name of the leveraged system.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="leveraged-authorization-service-provider">
      <xs:annotation>
         <xs:documentation>
            <b>Leveraged System Service Provider Name: </b>Name of the service provider that manages the leveraged system.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="leveraged-authorization-date-granted">
      <xs:annotation>
         <xs:documentation>
            <b>Leveraged System Authorization Date: </b>The date the leveraged system received its authorization.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="authorization-boundary">
      <xs:annotation>
         <xs:documentation>
            <b>Authorization Boundary: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:boundary-diagram"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="boundary-diagram">
      <xs:annotation>
         <xs:documentation>
            <b>Boundary Diagram: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:boundary-description"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="attachment-id" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Attachment ID / identifier: </b>Unique attachment identifier.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="boundary-description">
      <xs:annotation>
         <xs:documentation>
            <b>Boundary Description: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="oscal-ssp:prose" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="network-architecture">
      <xs:annotation>
         <xs:documentation>
            <b>Network Architecture Diagrams: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:network-diagram"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="network-diagram">
      <xs:annotation>
         <xs:documentation>
            <b>Network Diagram: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:network-description"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="attachment-id" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Attachment ID / identifier: </b>Unique attachment identifier.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="network-description">
      <xs:annotation>
         <xs:documentation>
            <b>Network Description: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="oscal-ssp:prose" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="data-flow">
      <xs:annotation>
         <xs:documentation>
            <b>Data Flow: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:data-flow-diagram"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="data-flow-diagram">
      <xs:annotation>
         <xs:documentation>
            <b>Data Flow Diagram: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:data-flow-description"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="attachment-id" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Attachment ID / identifier: </b>Unique attachment identifier.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="data-flow-description">
      <xs:annotation>
         <xs:documentation>
            <b>Data Flow Description: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="oscal-ssp:prose" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="users">
      <xs:annotation>
         <xs:documentation>
            <b>Users: </b>Provides all user roles.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:role"/>
            <xs:element minOccurs="0" ref="oscal-ssp:statistics"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="role">
      <xs:annotation>
         <xs:documentation>
            <b>Role: </b>Defining a role to be assigned to a party or agent</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:title"/>
            <xs:element minOccurs="0" ref="oscal-ssp:short-name"/>
            <xs:element minOccurs="0" ref="oscal-ssp:desc"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:privilege"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:responsibility"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID" use="required">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Name: </b>Name allows the information to be named.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="external" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  <b>external: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="access" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>access: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="sensitivity-level" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>sensitivity-level: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="privilege">
      <xs:annotation>
         <xs:documentation>
            <b>Privilege: </b>Identifies the type/level of system privilege for the role. 
			For FedRAMP, the only valid values are "p" (Privileged), "np" (Non-Privileged), or "nla" (No Logical Access).</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="responsibility">
      <xs:annotation>
         <xs:documentation>
            <b>Responsibility: </b>Identifies the functions performed by the user role.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="statistics">
      <xs:annotation>
         <xs:documentation>
            <b>Statistics: </b>Provides a collection of statistics about users of the system.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:internal-user-total-current"/>
            <xs:element minOccurs="0" ref="oscal-ssp:internal-user-total-future"/>
            <xs:element minOccurs="0" ref="oscal-ssp:external-user-total-current"/>
            <xs:element minOccurs="0" ref="oscal-ssp:external-user-total-future"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="internal-user-total-current">
      <xs:annotation>
         <xs:documentation>
            <b>Total Current Internal Users: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="internal-user-total-future">
      <xs:annotation>
         <xs:documentation>
            <b>Total Future Internal Users: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="external-user-total-current">
      <xs:annotation>
         <xs:documentation>
            <b>Total Current External Users: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="external-user-total-future">
      <xs:annotation>
         <xs:documentation>
            <b>Total Future External Users: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="system-implementation">
      <xs:annotation>
         <xs:documentation>
            <b>System Implementation: </b>Provides information as to how the system is implemented.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:ports-protocols-services"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:interconnection"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:component"/>
            <xs:element minOccurs="0" ref="oscal-ssp:system-inventory"/>
         </xs:sequence>
         <xs:attribute name="interconnected" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  <b>Interconnection status: </b>Are their system interconnections? (Yes/No)</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="ports-protocols-services">
      <xs:annotation>
         <xs:documentation>
            <b>Ports, Protocols, and Services: </b>A collection of the ports, protocols, and services used within the
			system.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:service"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="service">
      <xs:annotation>
         <xs:documentation>
            <b>Service: </b>A collection of information about an individual service within the
			system.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:protocol"/>
            <xs:element minOccurs="0" ref="oscal-ssp:purpose"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:used-by"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="name" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Name: </b>Name allows the information to be named.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="protocol">
      <xs:annotation>
         <xs:documentation>
            <b>Protocol: </b>Information about the protocol used to provide a service.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:port-range"/>
         </xs:sequence>
         <xs:attribute name="name" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Name: </b>Name allows the information to be named.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="port-range">
      <xs:annotation>
         <xs:documentation>
            <b>Port Range: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:attribute name="start" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Start: </b>Indicates the starting port number in a port range.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="end" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>End: </b>Indicates the ending port number in a port range.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="transport" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Transport: </b>Indicates the transport type.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="purpose">
      <xs:annotation>
         <xs:documentation>
            <b>Purpose: </b>Describes the purpose for the service within the system.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:group ref="oscal-ssp:everything-inline"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="used-by">
      <xs:annotation>
         <xs:documentation>
            <b>Used By: </b>Identifies the types of system component that uses the service. One entry per
		</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="interconnection">
      <xs:annotation>
         <xs:documentation>
            <b>Interconnection: </b>Details on an individual system interconnection.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:external-system-name"/>
            <xs:element minOccurs="0" ref="oscal-ssp:external-system-org"/>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:isa-authorization"/>
            <xs:element minOccurs="0" ref="oscal-ssp:isa-name"/>
            <xs:element minOccurs="0" ref="oscal-ssp:isa-date"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="external-system-name">
      <xs:annotation>
         <xs:documentation>
            <b>External System Name: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="external-system-org">
      <xs:annotation>
         <xs:documentation>
            <b>External System Organization: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="isa-authorization">
      <xs:annotation>
         <xs:documentation>
            <b>ISA Authorization: </b>Name of the individual who authorized the Interconnection Security Agreement
			(ISA).</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="isa-name">
      <xs:annotation>
         <xs:documentation>
            <b>ISA Name: </b>Title of the Interconnection Security Agreement (ISA).</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="isa-date">
      <xs:annotation>
         <xs:documentation>
            <b>ISA Date: </b>Date of the Interconnection Security Agreement (ISA).</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="component">
      <xs:annotation>
         <xs:documentation>
            <b>Component: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:origin"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:characteristics"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:satisfaction"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:validation"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:provisioning"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:subcomponent"/>
         </xs:sequence>
         <xs:attribute name="type" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Type: </b>Type designates the type of information provided.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="origin">
      <xs:annotation>
         <xs:documentation>
            <b>Origin: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="oscal-ssp:organization"/>
            <xs:element minOccurs="0" ref="oscal-ssp:title"/>
            <xs:element minOccurs="0" ref="oscal-ssp:version"/>
            <xs:element minOccurs="0" ref="oscal-ssp:release-date"/>
            <xs:element minOccurs="0" ref="oscal-ssp:model"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:part"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="vendor">
      <xs:annotation>
         <xs:documentation>
            <b>Vendor: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="type" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Type: </b>Type designates the type of information provided.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="release-date">
      <xs:annotation>
         <xs:documentation>
            <b>Release Date: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="model">
      <xs:annotation>
         <xs:documentation>
            <b>Model: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="part">
      <xs:annotation>
         <xs:documentation>
            <b>Part: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:phone"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:email"/>
         </xs:sequence>
         <xs:attribute name="class" type="xs:NMTOKEN">
            <xs:annotation>
               <xs:documentation>
                  <b>Class: </b>Identifies the property or object within the control; a semantic hint</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="href" type="xs:anyURI">
            <xs:annotation>
               <xs:documentation>
                  <b>Hypertext reference: </b>A relative or absolute URI indicating a resource available on the Internet.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="characteristics">
      <xs:annotation>
         <xs:documentation>
            <b>Characteristics: </b>Component characteristics, assets and configuration information</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:ip-address"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:service"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:part"/>
         </xs:sequence>
         <xs:attribute name="context" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Context: </b>Context designates the context of information provided.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="ip-address">
      <xs:annotation>
         <xs:documentation>
            <b>Internet Protocol Address: </b>Use the @type flag to deisgnate IPv4 or IPv6.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true">
         <xs:attribute name="type" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Type: </b>Type designates the type of information provided.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="satisfaction">
      <xs:annotation>
         <xs:documentation>
            <b>Satisfaction: </b>Describes how the indicated control is satisfied by this
			component</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:group ref="oscal-ssp:prose" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="context" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Context: </b>Context designates the context of information provided.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="system-inventory">
      <xs:annotation>
         <xs:documentation>
            <b>System Inventory: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:inventory-item"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="inventory-item">
      <xs:annotation>
         <xs:documentation>
            <b>Inventory Item: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:ip-address"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:dns-name"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:host-item"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:software-item"/>
            <xs:element minOccurs="0" ref="oscal-ssp:comments"/>
            <xs:element minOccurs="0" ref="oscal-ssp:serial-no"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:network-id"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:asset-owner"/>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:asset-administrator"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="component-id" type="xs:NMTOKEN">
            <xs:annotation>
               <xs:documentation>
                  <b>Component ID / identifier: </b>ID of a linked or imported component</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="virtual" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  <b>Virtual: </b>Designates an asset as either virtual (="yes") or physical
			(="no").</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="public" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>
                  <b>Public: </b>Designates an asset as either publicly accessible (="yes") or not
			(="no").</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="dns-name">
      <xs:annotation>
         <xs:documentation>
            <b>Network Name: </b>If available, state the DNS name or URL of the inventory item. This can be left
			blank if one does not exist, or it is a dynamic field.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="host-item">
      <xs:annotation>
         <xs:documentation>
            <b>Host Item: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:netbios-name"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:mac-address"/>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:authenticated-scan"/>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:baseline-template"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:os-name"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:os-version"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:location"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:asset-type"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:vendor"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:hardware-model"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:scanned"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:prop"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="netbios-name">
      <xs:annotation>
         <xs:documentation>
            <b>NetBIOS Name: </b>If available, state the NetBIOS name of the inventory item. This can be omitted
			if one does not exist, or it is a dynamic field.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="mac-address">
      <xs:annotation>
         <xs:documentation>
            <b>Media Access Control (MAC) Address: </b>If available, state the MAC Address of the inventory item. This can be left
			blank if one does not exist, or it is a dynamic field.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="os-name">
      <xs:annotation>
         <xs:documentation>
            <b>Operating System Name: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="os-version">
      <xs:annotation>
         <xs:documentation>
            <b>Operating System Version Number: </b>.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="location">
      <xs:annotation>
         <xs:documentation>
            <b>Location of the Asset: </b>Physical location of hardware. Could include Data Center ID, Cage#, Rack# or
			other meaningful location identifiers.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="asset-type">
      <xs:annotation>
         <xs:documentation>
            <b>Asset Type: </b>Simple description of the asset's function (e.g., Router, Storage Array, DNS
			Server, etc.) Do not use vendor or product names here.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="hardware-model">
      <xs:annotation>
         <xs:documentation>
            <b>Hardware Model: </b>Model number of the hardware.</xs:documentation>
      </xs:annotation>
      <xs:complexType mixed="true"/>
   </xs:element>
   <xs:element name="authenticated-scan" type="xs:boolean">
      <xs:annotation>
         <xs:documentation>
            <b>Authenticated Scan: </b>Indicate whether this asset is planned for authenticated
			scanning.</xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:element name="software-item">
      <xs:annotation>
         <xs:documentation>
            <b>Software Item: </b>
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:vendor"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:software-name"/>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:software-version"/>
            <xs:element maxOccurs="unbounded"
                        minOccurs="0"
                        ref="oscal-ssp:software-patch-level"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="oscal-ssp:function"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="software-name">
