         </xs:sequence>
         <xs:attribute name="id" type="xs:ID">
            <xs:annotation>
               <xs:documentation>
                  <b>Component identification number.: </b>.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="ref-type" type="xs:string">
            <xs:annotation>
               <xs:documentation>
                  <b>Reference Type: </b>Identifies the reference type, such as a law, standard, guidance.</xs:documentation>
