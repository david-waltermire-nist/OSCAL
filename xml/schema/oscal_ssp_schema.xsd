                        minOccurs="0"
                        ref="oscal-ssp:control-response"/>
         </xs:sequence>
         <xs:attribute name="class" type="xs:NMTOKEN">
            <xs:annotation>
               <xs:documentation>
                  <b>Class: </b>Identifies the property or object within the control; a semantic hint</xs:documentation>
